<%- include('_header.ejs') -%>
<form method="POST" action="/signup" >
    <div class="form-group">
      <label for="Inputusername">username</label>
      <input type="text" class="form-control" name="username" id="exampleInputusername" aria-describedby="emailHelp" placeholder="Enter username">
    </div>
    <div class="form-group">
      <label for="InputPassword1">Password</label>
      <input type="password" class="form-control" name="passwordHash" id="exampleInputPassword1" placeholder="Password">
    </div>
    <div class="form-group">
        <label for="InputEmail1">Email address</label>
        <input type="email" class="form-control" name="email" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Email address">
      </div>

    <button type="submit" class="btn btn-primary">회원가입</button>
  </form>
<%- include('footer.ejs') -%>